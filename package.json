{
  "name": "@instantlyeasy/claude-code-sdk-ts",
  "version": "0.1.2",
  "description": "Unofficial TypeScript port of the official Python Claude Code SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "sdk",
    "typescript"
  ],
  "author": "Daniel King & Claude",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "execa": "^8.0.1",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/which": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/instantlyeasy/claude-code-sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/instantlyeasy/claude-code-sdk-ts/issues"
  },
  "homepage": "https://github.com/instantlyeasy/claude-code-sdk-ts#readme"
}
